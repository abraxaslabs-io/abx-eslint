{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Next.js",
  "extends": "./base.json",
  "compilerOptions": {
    "plugins": [
      {
        "name": "next"
      }
    ],

    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "preserve",
    "target": "ES2022",
    "lib": ["dom", "dom.iterable", "esnext"],
    "checkJs": false
  }
}
